<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.patels95.sanam.ewb" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ewb_icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.Ewb" >
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.HomeOldActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".ui.SignUpActivity"
            android:label="@string/title_activity_signUp" >
        </activity>
        <activity
            android:name=".ui.ProjectsActivity"
            android:label="@string/title_activity_projects" >
        </activity>

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="3de5a9c20fda241f408ac7d972ebe00dfcceb2db" />

        <activity
            android:name=".ui.HomeActivity"
            android:label="@string/title_activity_home"
            android:parentActivityName=".ui.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.patels95.sanam.ewb.ui.MainActivity" />
        </activity>
    </application>

</manifest>
